{
  "name": "Monte-Carlo-simulations",
  "version": "1.0.0",
  "description": "Web application to generate Monte Carlo simulations for Kanban",
  "main": "index.html",
  "scripts": {
    "clean": "gulp clean",
    "pretest": "gulp compile-test",
    "jest": "node ./packages/jest-cli/bin/jest.js",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/tests/Results",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testPathDirs": [
      "<rootDir>/tests/JsOutput/tests"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/Results"
    ],
    "testRegex": "(<rootDir>/tests/JsOutput/tests/.*|(\\.|/)(test|spec))\\.jsx?$"
  },
  "dependencies": {
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "del": "^2.2.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chart.js": "^1.1.1",
    "fbjs": "^0.8.8",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-typescript": "^3.1.3",
    "gulp-util": "^3.0.7",
    "gulp-webserver": "^0.9.1",
    "jest": "^18.1.0",
    "jest-cli": "^18.0.0",
    "react-chartjs": "^0.8.0",
    "react-test-renderer": "^15.4.1",
    "requirejs": "^2.3.2",
    "ts-jest": "^18.0.1",
    "typescript": "^2.1.4"
  },
  "author": "Louis-Philippe Carignan",
  "license": "ISC"
}
